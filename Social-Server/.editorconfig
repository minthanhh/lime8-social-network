# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true
[*]
end_of_line = lf
indent_style = space
indent_size = 2
insert_final_newline = true
charset = utf-8
trim_trailing_whitespace = true

[*.{ts, js}]
quote_type = single

[*.md]
max_line_length = off
trim_trailing_whitespace = false
